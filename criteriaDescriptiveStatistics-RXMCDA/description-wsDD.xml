<?xml version="1.0" encoding="iso-8859-1"?>
<program_description xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.decision-deck.org/ws/_downloads/description.xsd">
    <program provider="RXMCDA"
        name="criteriaDescriptiveStatistics"
        displayName="criteriaDescriptiveStatistics"
        version="1.0" />
    <documentation>
        <description>Calculates elementary descriptive statistics on the criteria of a performance table (min, max, mean, median, standard deviation, variance, Pearson correlation).</description>
        <contact>Patrick Meyer (patrick.meyer@telecom-bretagne.eu).</contact>
    </documentation>
    
    
    <parameters>
        
        
        
        
        <input id="in1" 
            name="criteria"
            displayName="criteria"
            isoptional="0">
            <documentation>
                <description>A list of criteria. Criteria can be activated or desactivated via the &lt;active&gt; tag (true or false). By default (no &lt;active&gt; tag), criteria are considered as active. </description>
            </documentation>
            <xmcda tag="criteria">
                <![CDATA[
                    <criteria>
                        <criterion id="...">
                            <active>[...]</active>
                            [...]
                        </criterion>
                        [...]
                    </criteria>
                    ]]>
            </xmcda>
        </input>
        
        
        <input id="in2" 
            name="alternatives"
            displayName="alternatives"
            isoptional="0">
            <documentation>
                <description>A list of alternatives. Alternatives can be activated or desactivated via the &lt;active&gt; tag (true or false). By default (no &lt;active&gt; tag), alternatives are considered as active. </description>
            </documentation>
            <xmcda tag="criteria">
                <![CDATA[
                    <alternatives>
                        <alternative id="...">
                            <active>[...]</active>
                            [...]
                        </alternative>
                        [...]
                    </alternatives>
                    ]]>
            </xmcda>
        </input>
        
        <input id="in3" 
            name="performanceTable"
            displayName="performanceTable"
            isoptional="0">
            <documentation>
                <description>A performance table representing the performances of the alternatives on the criteria.</description>
            </documentation>
            <xmcda tag="performanceTable"/>
        </input>
        
        <output id="out1"
            name="messages"
            displayName="messages">
            
            <documentation>
                <description>A list of messages generated by the algorithm.</description>
            </documentation>
            <xmcda tag="methodMessages"/>
        </output>
        
        <output id="out2" 
            name="means"
            displayName="means">
            <documentation>
                <description>The means of the criteria of the performance table.</description>
            </documentation>
            <xmcda tag="criteriaValues">
                <![CDATA[
                    <criteriaValues>
                       <criterionValue>
                           <criterionID>[...]</criterionID>
                               <value>
                                   <real>[...]</real>
                                </value>
                        </criterionValue>
                        [...]
                    </criteriaValues>
                    ]]>
            </xmcda>
        </output>
        
        <output id="out3" 
            name="medians"
            displayName="medians">
            <documentation>
                <description>The medians of the criteria of the performance table.</description>
            </documentation>
            <xmcda tag="criteriaValues">
                <![CDATA[
                    <criteriaValues>
                       <criterionValue>
                           <criterionID>[...]</criterionID>
                               <value>
                                   <real>[...]</real>
                                </value>
                        </criterionValue>
                        [...]
                    </criteriaValues>
                    ]]>
            </xmcda>
        </output>
        
        <output id="out4" 
            name="mins"
            displayName="minima">
            <documentation>
                <description>The minimum values of the criteria of the performance table.</description>
            </documentation>
            <xmcda tag="criteriaValues">
                <![CDATA[
                    <criteriaValues>
                       <criterionValue>
                           <criterionID>[...]</criterionID>
                               <value>
                                   <real>[...]</real>
                                </value>
                        </criterionValue>
                        [...]
                    </criteriaValues>
                    ]]>
            </xmcda>
        </output>
        
        <output id="out5" 
            name="maxes"
            displayName="maxima">
            <documentation>
                <description>The maximum values of the criteria of the performance table.</description>
            </documentation>
            <xmcda tag="criteriaValues">
                <![CDATA[
                    <criteriaValues>
                       <criterionValue>
                           <criterionID>[...]</criterionID>
                               <value>
                                   <real>[...]</real>
                                </value>
                        </criterionValue>
                        [...]
                    </criteriaValues>
                    ]]>
            </xmcda>
        </output>
        
        <output id="out6" 
            name="standardDeviations"
            displayName="standardDeviations">
            <documentation>
                <description>The standard deviation values of the criteria of the performance table.</description>
            </documentation>
            <xmcda tag="criteriaValues">
                <![CDATA[
                    <criteriaValues>
                       <criterionValue>
                           <criterionID>[...]</criterionID>
                               <value>
                                   <real>[...]</real>
                                </value>
                        </criterionValue>
                        [...]
                    </criteriaValues>
                    ]]>
            </xmcda>
        </output>
        
        <output id="out7" 
            name="variances"
            displayName="variances">
            <documentation>
                <description>The variance values of the criteria of the performance table.</description>
            </documentation>
            <xmcda tag="criteriaValues">
                <![CDATA[
                    <criteriaValues>
                       <criterionValue>
                           <criterionID>[...]</criterionID>
                               <value>
                                   <real>[...]</real>
                                </value>
                        </criterionValue>
                        [...]
                    </criteriaValues>
                    ]]>
            </xmcda>
        </output>
        
        <output id="out8" 
            name="correlation"
            displayName="correlations">
            <documentation>
                <description>The pairwise Pearson correlations between the criteria criteria of the performance table.</description>
            </documentation>
            <xmcda tag="criteriaValues">
                <![CDATA[
                    <criteriaValues>
                       <criterionValue>
                           <criterionID>[...]</criterionID>
                               <value>
                                   <real>[...]</real>
                                </value>
                        </criterionValue>
                        [...]
                    </criteriaValues>
                    ]]>
            </xmcda>
        </output>
        
    </parameters>
</program_description>

